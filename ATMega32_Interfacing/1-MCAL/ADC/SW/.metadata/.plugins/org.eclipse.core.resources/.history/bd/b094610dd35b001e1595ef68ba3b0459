#ifndef ADC_REGISTER_H_
#define ADC_REGISTER_H_

#define ADMUX    *((volatile uint8*) 0x27)

#define ADCSRA   *((volatile uint8*) 0x26)

#define ADCH     *((volatile uint8*) 0x25)

#define ADCL     *((volatile uint8*) 0x24)


#endif
