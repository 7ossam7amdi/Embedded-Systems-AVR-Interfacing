#ifndef EXTINT_CONFIG_H_
#define EXTINT_CONFIG_H_


/* Configure INT0 or INT1 Sense Control */
/* Choose one of these States:
 * - FALLING_EDGE
 * - RISING_EDGE
 * - ON_CHANGE
 * - LOW_LEVEL
*/

#define INT0_SENSE	LOW_LEVEL

#define INT1_SENSE	FALLING_EDGE

/* Configure INT2Sense Control */
/* Choose one of these States:
 * - FALLING_EDGE
 * - RISING_EDGE
*/

#define INT2_SENSE	FALLING_EDGE


#endif
