#ifndef SPI_REGISTER_H_
#define SPI_REGISTER_H_


#define SPDR			*((volatile uint8*) 0x2F)
#define SPSR			*((volatile uint8*) 0x2E)
#define SPCR			*((volatile uint8*) 0x2D)






#endif
