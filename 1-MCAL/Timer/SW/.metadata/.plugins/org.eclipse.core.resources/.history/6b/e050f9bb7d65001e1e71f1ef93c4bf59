#include "LIB/Std_Types.h"
#include "DIO/DIO_interface.h"
#include "Timer_interface.h"


int main(){
	DIO_SetPinDirection(PORT_D, PIN5, OUTPUT_PIN);
	Timer1_Init();

	while(1){
		for(int i=1000 ; i< 2000 ; i+=100)
		Servo_Go(i);
	}
}
