#ifndef SSD_PRIVATE_H_
#define SSD_PRIVATE_H_

#define Anode_0		0b1000000
#define Anode_1		0b1111001
#define Anode_2		0b0100100
#define Anode_3		0b0110000
#define Anode_4		0b0011001
#define Anode_5		0b0010010
#define Anode_6		0b0000010
#define Anode_7		0b1111000
#define Anode_8		0b0000000
#define Anode_9		0b0010000


#endif
